#define main.h "/root/alx-low_level_programming/0x02-functions_nested_loops/main.h"
#define headers.h "/root/alx-low_level_programming/0x02-functions_nested_loops/headers.h"
#include "headers.h"
#include "main.h"
/**
 * print_alphabet - prints alphabet in lowercase, followed by a new line
 *
 **/
void print_alphabet(void)
{
	char c;

	for (c = 'a'; c <= 'z'; c++)
	{
		_putchar(c);
		_putchar('\n');
	}
}
